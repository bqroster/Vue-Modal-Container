{
  "name": "bqr-vue-modal-container",
  "version": "0.1.0",
  "private": false,
  "description": "Vue Modal Container to handle Modal and Notifications package",
  "author": "Jose Burgos <jose@bqroster.com>",
  "license": "MIT",
  "main": "dist/bqr-vue-modal-container.umd.js",
  "module": "dist/bqr-vue-modal-container.esm.js",
  "unpkg": "dist/bqr-vue-modal-container.min.js",
  "browser": {
    "./sfc": "src/ModalContainer.vue"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bqroster/Vue-Modal-Container.git"
  },
  "homepage": "https://github.com/bqroster/Vue-Modal-Container",
  "bugs": {
    "url": "https://github.com/bqroster/Vue-Modal-Container/issues"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "Modal",
    "Notification",
    "Javascript"
  ],
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "cross-env NODE_ENV=production rollup --config rollup.config.js --format umd --file dist/bqr-vue-modal-container.umd.js",
    "build:es": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es --file dist/bqr-vue-modal-container.esm.js",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife --file dist/bqr-vue-modal-container.min.js",
    "test": "jest",
    "lint": "eslint --ext .js,.vue src"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^4.14.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue2": "^0.8.1",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "dependencies": {}
}
